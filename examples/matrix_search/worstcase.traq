fn Oracle(x: Fin<N>, y: Fin<M>) do
	ze <- const 1 : Fin<M>;
	ok <- ze <= y;
	return ok : Bool
end

fn IsEntryZero(i0: Fin<N>, j0: Fin<M>) do
	e <- Oracle(i0, j0);
	e' <- !e;
	return e' : Bool
end

fn IsRowAllOnes(i: Fin<N>) do
	hasZero <- @any[IsEntryZero](i);
	okr <- not hasZero;
	return okr : Bool
end

fn HasAllOnesRow() do
	ok <- @any[IsRowAllOnes]();
	return ok : Bool
end
