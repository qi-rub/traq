declare Oracle(Fin<N>, Fin<M>) -> Bool

def IsEntryZero(i: Fin<N>, j: Fin<M>) do
	e <- Oracle(i, j);
	e' <- !e;
	return e' : Bool
end

def IsRowAllOnes(i: Fin<N>) do
	ok <- any(IsEntryZero, i);
	ok' <- !ok;
	return ok' : Bool
end

def HasAllOnesRow() do
	ok <- any(IsRowAllOnes);
	return ok : Bool
end

result <- HasAllOnesRow()
