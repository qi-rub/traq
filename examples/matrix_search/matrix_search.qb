declare Matrix(Fin<N>, Fin<M>) -> Bool end

def IsEntryZero(i0: Fin<N>, j0: Fin<M>) -> Bool do
	e <- Matrix(i0, j0);
	e' <- !e;
	return e'
end

def IsRowAllOnes(i: Fin<N>) -> Bool do
	hasZero <- @any[IsEntryZero](i);
	okr <- not hasZero;
	return okr
end

def HasAllOnesRow() -> Bool do
	ok <- @any[IsRowAllOnes]();
	return ok
end
