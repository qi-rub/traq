declare Oracle(Fin<N>, Fin<M>) -> Bool

def IsEntryZero(i0: Fin<N>, j0: Fin<M>) do
	e <- Oracle(i0, j0);
	e' <- !e;
	return e' : Bool
end

def IsRowAllOnes(i: Fin<N>) do
	okr <- @any(IsEntryZero, i);
	okr' <- !okr;
	return okr' : Bool
end

def HasAllOnesRow() do
	ok <- @any(IsRowAllOnes);
	return ok : Bool
end

result <- HasAllOnesRow()
